<h1>Listing photo1s</h1>

<table>
  <tr>
    <th>Image name</th>
    <th>Image description</th>
    <th>Image type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photo1s.each do |photo1| %>
  <tr>
    <td><%= photo1.image_name %></td>
    <td><%= photo1.image_description %></td>
    <td><%= photo1.image_type %></td>
    <td><%= link_to 'Show', photo1 %></td>
    <td><%= link_to 'Edit', edit_photo1_path(photo1) %></td>
    <td><%= link_to 'Destroy', photo1, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo1', new_photo1_path %>
