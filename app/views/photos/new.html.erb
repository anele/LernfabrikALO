<h1>New photo</h1>

<%= form_for(@photo) do |f| %>

  <p>
    <%= f.label :image_name %><br />
    <%= f.text_field :image_name %>
  </p>
  <p>
    <%= f.label :image_description %><br />
    <%= f.text_field :image_description %>
  </p>
  <p>
    <%= f.label :image_type %><br />
    <%= f.text_field :image_type %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>
<%= link_to 'Back', photos_path %>
